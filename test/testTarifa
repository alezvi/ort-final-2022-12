const axios = require('axios')
const chai = require('chai');
const { assert } = chai;

describe('Get Tarifas', () => {

    it('return 200 if get tarifas', (done) => {
        axios({
            method: 'get',
            url: 'http://localhost:5555/tarifas/',
        }).then(response => {
            assert.equal(response.status, 200)
            done()
        }).catch(err => {
            console.log(err.message)
        })
    })
});